{
  "name": "doubleeue",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "compile:server": "webpack --config webpack.server.config.js --progress --colors",
    "serve:ssr": "node dist/server",
    "build:ssr": "npm run build:client-and-server-bundles && npm run compile:server",
    "build:client-and-server-bundles": "ng build --prod && ng run doubleeue:server:production --bundleDependencies all",
    "dev": "ng build --watch --deploy-url /wordpress/wp-content/themes/doubleeue/dist/",
    "prod": "ng build --prod --baseHref /wordpress/wp-content/themes/doubleeue/dist/"
  },
  "private": true,
  "dependencies": {
    "@angular-devkit/schematics": "^10.0.5",
    "@angular/animations": "^10.0.8",
    "@angular/cdk": "~10.1.2",
    "@angular/common": "~10.0.8",
    "@angular/compiler": "~10.0.8",
    "@angular/core": "~10.0.8",
    "@angular/forms": "~10.0.8",
    "@angular/material": "^10.1.2",
    "@angular/platform-browser": "~10.0.8",
    "@angular/platform-browser-dynamic": "~10.0.8",
    "@angular/platform-server": "~10.0.8",
    "@angular/router": "~10.0.8",
    "@nguniversal/express-engine": "^10.0.1",
    "@nguniversal/module-map-ngfactory-loader": "8.2.6",
    "express": "^4.15.2",
    "ngx-owl-carousel-o": "^4.0.0",
    "rxjs": "~6.6.2",
    "tslib": "^2.0.1",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1000.5",
    "@angular/compiler-cli": "~10.0.8",
    "@angular/language-service": "~10.0.8",
    "@types/jasmine": "~3.5.12",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^14.0.27",
    "codelyzer": "^6.0.0",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.2",
    "karma": "~5.1.1",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.3",
    "karma-jasmine": "~3.3.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "protractor": "~7.0.0",
    "ts-loader": "^8.0.2",
    "ts-node": "~8.10.2",
    "tslint": "~6.1.3",
    "typescript": "~3.9.7",
    "webpack-cli": "^3.1.0"
  }
}
